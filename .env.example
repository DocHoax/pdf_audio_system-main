# EchoDoc Environment Configuration
# Copy this file to .env and fill in your values

# App Environment (local or production)
APP_ENV=local

# Database Configuration
DB_HOST=localhost
DB_NAME=echodoc_db
DB_USER=root
DB_PASS=
DB_PORT=3306

# YarnGPT API
YARNGPT_API_KEY=your_yarngpt_api_key_here

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost/pdf_audio_system-main/google-callback.php
